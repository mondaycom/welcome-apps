{
  "name": "monday-code-github-integration-template",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "start": "npm run server",
    "dev": "npm run dev:stop && concurrently \"npm run dev:server\" \"npm run dev:expose\"",
    "dev:server": "nodemon --inspect=8080 ./src/app.js",
    "dev:expose": "mapps tunnel:create -p 8080",
    "dev:stop": "kill-port 8080",
    "server": "NODE_ENV=production node ./src/app.js",
    "deploy": "npm run mapps-updated code:push",
    "logs": "npm run mapps-updated code:logs",
    "mapps:init": "npm run mapps-updated init",
    "mapps-updated": "mapps -u"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^0.1.3",
    "@octokit/rest": "^16.43.2",
    "body-parser": "^1.19.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "monday-sdk-js": "^0.1.4",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "simple-oauth2": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^v6.8.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.2"
  }
}
