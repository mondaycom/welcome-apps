{
  "name": "monday-lifecycle-events-app",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "description": "Example app for handling Monday.com app feature lifecycle events",
  "scripts": {
    "dev": "concurrently \"ts-node-dev --watch ./.env ./src/app.ts\" \"npm run expose\"",
    "start": "ts-node ./src/app.ts",
    "expose": "mapps tunnel:create -p 8080",
    "init": "mapps init"
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^3.2.1",
    "body-parser": "^1.19.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.6.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^4.10.4",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-fetch": "^2.6.11",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "nodemon": "^3.0.2",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
