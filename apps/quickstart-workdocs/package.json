{
  "name": "monday-workdocs-quickstart-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run init && npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "init": "mapps init -l",
    "expose": "mapps tunnel:create -p 8306",
    "server": "vite",
    "deploy:build": "vite build",
    "deploy:push": "mapps code:push --client-side -d \"build\"",
    "deploy": "npm run deploy:build && npm run deploy:push",
    "stop": "kill-port 8306 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^2.1.2",
    "@vibe/core": "^3.19.0",
    "monday-sdk-js": "^0.5.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^4.9.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "eslint-config-react-app": "^7.0.1",
    "ngrok": "^4.3.3",
    "quill-delta": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.8",
    "vite": "^6.2.2"
  }
}
