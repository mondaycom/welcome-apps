    "lodash": "^4.17.21",
    "monday-sdk-js": "^0.5.2",
    "monday-ui-react-core": "^2.139.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@mondaycom/apps-cli": "^4.2.0",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.2.1",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0"
  },
  "eslintConfig": {
