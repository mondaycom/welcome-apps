{
  "name": "monday-example-app-word-cloud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "react-scripts start",
    "build": "react-scripts build",
    "expose": "mtunnel -p 8303",
    "stop": "kill-port 8303 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "lodash": "^4.17.21",
    "monday-sdk-js": "^0.3.0",
    "monday-ui-react-core": "^1.102.1",
    "react": "^16.10.2",
    "react-d3-cloud": "^1.0.5",
    "react-dom": "^16.10.2",
    "react-scripts": "^5.0.1",
    "react-wordcloud": "^1.1.1"
  },
  "devDependencies": {
    "@mondaydotcomorg/tunnel": "^0.4.1",
    "concurrently": "^4.1.2",
    "cross-port-killer": "^1.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
