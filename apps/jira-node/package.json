{
  "name": "monday-jira-integration",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "npm run stop && npm run db:migrate && concurrently \"npm run server\" \"npm run expose\"",
    "server": "nodemon ./src/app.js",
    "expose": "mapps tunnel:create -p 8302",
    "db:migrate": "sequelize db:migrate",
    "stop": "kill-port 8302"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-port-killer": "^1.2.1",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "monday-sdk-js": "^0.1.4",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "sequelize": "^6.30.0",
    "simple-oauth2": "^4.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@mondaydotcomorg/tunnel": "^0.4.1",
    "concurrently": "^8.2.2",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^6.2.0"
  }
}
