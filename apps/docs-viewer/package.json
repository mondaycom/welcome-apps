{
  "name": "monday-example-app-docs-viewer",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "monday-sdk-js": "^0.1.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-modal": "^3.11.2",
    "react-onclickoutside": "^6.9.0",
    "react-scripts": "^5.0.1",
    "request": "^2.88.2",
    "sass": "^1.54.8",
    "typeface-roboto": "^0.0.75",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^2.1.1",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.2.1"
  },
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "react-scripts start",
    "build": "react-scripts build",
    "expose": "mapps tunnel:create -p 8304",
    "stop": "kill-port 8304 && kill-port 4049 && kill-port 4040"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-loader": "8.0.6"
  }
}
